{
  "builds": [
    {
      "src": "alginos/wsgi.py",
      "use": "@vercel/python",
      "config": {"maxLambdaSize" : "15mb", "runtime": "python3.9"}
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "alignos/wsgi.py"
    }
  ],
  "rewrites": [{ "source": "/(.*)", "destination": "/" }]
}